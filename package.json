    "scripts": {
        "build": "PUBLIC_PATH=https://suite.camino.network/wallet/ webpack --config webpack.prod.js",
        "build:dev": "PUBLIC_PATH=https://dev.suite.camino.network/wallet/ webpack --config webpack.dev.js",
        "build:stage": "PUBLIC_PATH=https://stage.suite.camino.network/wallet/ webpack --config webpack.prod.js",
        "build:start": "cd dist && PORT=5003 npx serve",
        "start": "webpack-dev-server --config webpack.local.js",
        "start:live": "webpack-dev-server --open --config webpack.local.js --live-reload",
        "dev": "webpack-dev-server --mode development --open",
        "cypress": "cypress run",
        "cypress:columbus": "TEST_ENV=columbus cypress run",
        "format": "prettier src/**/*.{js,ts,vue} .github/workflows/*.yml --write",
        "format-check": "prettier src/**/*.{js,ts,vue} .github/workflows/*.yml --check",
        "lint": "vue-cli-service lint",
        "release": "release-it",
        "test_watch": "jest --verbose --watch ./tests",
        "test_debug": "node --inspect-brk node_modules/.bin/jest ./tests --runInBand",
        "test": "jest --verbose ./tests",
        "preinstall": "git submodule update --recursive --init"
    },
